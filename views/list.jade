extends layout

block content
  #container
    div
      h1= title
      // ul
        each val, index in ['zero', 'one', 'two']
          li= index + ': ' + val
      // ul
        each val, index in coffee.coffee
          li= 'Id: ' + val.id + ' Title: ' + val.title
      ul
        each val in coffee.coffee
          li
            a(href='product?id=#{val.id}') #{val.roastery} - #{val.title}